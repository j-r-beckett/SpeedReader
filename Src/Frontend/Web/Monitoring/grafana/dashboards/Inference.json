                    "rawSql": "SELECT\n      time_bucket('5s', time) AS time,\n      metric_name AS metric,\n      percentile_cont(0.5) WITHIN GROUP (ORDER BY value) AS \"P50\",\n      percentile_cont(0.9) WITHIN GROUP (ORDER BY value) AS \"P90\",\n      percentile_cont(0.9999) WITHIN GROUP (ORDER BY value) AS \"P99.99\"\n  FROM metrics\n  WHERE $__timeFilter(time) \n    AND metric_name = 'speedreader.inference.duration'\n    AND tags->>'model' = '$model'\n  GROUP BY time_bucket('5s', time), metric_name\n  ORDER BY time;",
